<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name ="Test NG Demo">
    <test name ="multiple tests" enabled="false">
        <classes>
            <class name="org.example.RestTest">
                <methods>
                    <include name="testGetUserName"/>
                    <include name="testUpdatePartial"/>
                    <include name="testPostUser"/>
                    <include name ="jsonUser"/>
                    <include name="testDelete"/>
                    <include name="testDeletedBooking"/>

                </methods>
            </class>
        </classes>>
    </test>
    <test name ="RestfulBookerTest" enabled="true">
        <classes>
            <class name="org.example.RestfulBookerTest">
                <methods>
                    <include name="testCreateBooking"/>
                    <include name ="getBooking"/>
                    <include name ="testGenerateToken"/>
                    <include name ="testUpdateBooking"/>
                    <include name ="testPartialUpdate"/>
                    <include name ="testDelete"/>
                    <include name ="testDeletedBooking"/>
                </methods>
            </class>
        </classes>>
    </test>
</suite>